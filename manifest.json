{
  "name": "Tradingworks Plus",
  "version": "0.0.1",
  "description": "Tradingworks",
  "icons": { 
    "16": "assets/favicon16.png",
    "32": "assets/favicon32.png",
    "48": "assets/favicon48.png"
  },
  "manifest_version": 3,
  "permissions": ["activeTab", "tabs", "storage", "scripting", "notifications"],
  "host_permissions": ["https://app.tradingworks.net/*", "https://api.callmebot.com/*"],
  "action": {
    "default_popup": "./popup.html",
    "default_title": "Tradingworks"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "teste",
      "strict_min_version": "42.0"
    }
  }
}